{
  "name": "radial",
  "version": "handled by scripts/update-versions.sh",
  "description": "An Obsidian plugin that adds a radial/pie menu for quick actions/action groups for better UX on mobile, and (to a lesser extent) desktop.",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [],
  "author": "Garrett Swann",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "8.49.0",
    "@typescript-eslint/parser": "8.49.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "esbuild-svelte": "^0.9.3",
    "eslint": "^9.39.1",
    "eslint-plugin-obsidianmd": "^0.1.9",
    "eslint-plugin-svelte": "^3.13.1",
    "obsidian": "latest",
    "svelte": "^5.45.4",
    "svelte-check": "^4.3.4",
    "svelte-preprocess": "^6.0.3",
    "tslib": "2.4.0",
    "typescript": "~5.0.0",
    "typescript-eslint": "^8.49.0"
  },
  "type": "module"
}
